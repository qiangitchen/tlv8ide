<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.tulin.v8.fn.FnEditor"
            contributorClass="com.tulin.v8.fn.XMLMultiPageEditorContributor"
            extensions="fn,fn.xml"
            default="true"
            icon="icons/fun.gif"
            id="com.tulin.v8.fn.FnEditor"
            name="%editor.name.fn">
         <contentTypeBinding
               contentTypeId="com.tulin.v8.editors.fn.contenttype">
         </contentTypeBinding>
      </editor>
   </extension>
   
   <extension
       point="org.eclipse.core.contenttype.contentTypes">
	    <content-type
           base-type="org.eclipse.core.runtime.xml"
           file-extensions="fn,xml"
           file-patterns="*.fn,*.fn.xml"
           id="com.tulin.v8.editors.fn.contenttype"
           name="Fn"
           priority="normal">
	    </content-type>
   </extension>

</plugin>
