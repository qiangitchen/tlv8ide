<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension
       point="org.eclipse.core.contenttype.contentTypes">
	    <content-type
           base-type="org.eclipse.wst.html.core.htmlsource"
           file-extensions="htm,html,xhtml"
           id="com.tulin.v8.editors.html.contenttype"
           name="HTML"
           priority="normal">
	    </content-type>
   </extension>
   
   <extension
       point="org.eclipse.core.contenttype.contentTypes">
	    <content-type
           base-type="org.eclipse.wst.css.core.csssource"
           file-extensions="css"
           id="com.tulin.v8.editors.css.contenttype"
           name="CSS"
           priority="normal">
	    </content-type>
   </extension>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.tulin.v8.editors.page.WebPageEditor"
            contributorClass="com.tulin.v8.editors.page.MultiPageEditorContributor"
            default="true"
            extensions="htm,html,xhtml"
            icon="icons/page.gif"
            id="com.tulin.v8.editors.page.WebPageEditor"
            name="%editor.name.page">
      </editor>
   </extension>

</plugin>
